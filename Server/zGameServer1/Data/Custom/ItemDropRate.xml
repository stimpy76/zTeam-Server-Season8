<itemdroprate>
	<!--
	type:		Item type (Item.txt)
	index:		Item index (Item.txt)
	minlvl:		Item min. level
	maxlvl: 	Item max. level
	skill:		Item skill option (0: without, 1: 50% chance)
	luck:		Item luck option (0: without, 1: 50% chance)
